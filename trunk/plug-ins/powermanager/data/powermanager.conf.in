#!en;@VERSION@

[Icon]

#F[Desired icon size for this applet]
frame1 = 
#i width :
width = 48
#i height :
height = 48
#F
frame1_fin = 

#s Name of the icon as it will appear in its label in the dock :
name = Battery


[Configuration]

#F[Quick info]
frame2=
#s[charge;time;nothing] Information to be displayed on the icon :
quick_info_type=charge

#F[Warnings]
frame3=
#b Warn me when the battery is charged ?
high_battery_witness=true
#b Warn me when the battery is low ?
low_battery_witness=true
#i Low battery threshold (in minutes) :
low_battery_value=15
