#!en;@VERSION_POWERMANAGER@

#[gtk-about]
[Icon]

#j+[0;128] Desired icon size for this applet
#{Set to 0Â to use the default applet size}
icon size = 0;0

#s Icon name :
name = Battery

#d Name of the dock it belongs to:
dock name = 

icon=
order=

#[gtk-convert]
[Desklet]

#j+[48;512] Desklet's dimension (width x height) :
#{Depending on your WindowManager, you can resize it with ALT + middle_click or ALT + left_click for exemple.}
size = 96;96

#i[-2048;2048] Desklet's position (x ; y) :
#{Depending on your WindowManager, you can move it with ALT + left_click}
x position=0
#i[-2048;2048] ...
y position=0

#b Is detached from the dock ?
initially detached=false

#b Keep below other windows ?
keep below=false
#b Keep above other windows ?
keep above=false
#b Keep on widget layer ?
#{for CompizFusion's "widget layer", set behaviour in Compiz to: (class=cairo-dock & type=utility)}
on widget layer=false
#b Lock position ?
#{If locked, the desklet can't be moved by simply dragging it with the left mouse button. Of course you can still move it with ALT + left_click.}
locked = false

#I[-180;180] Rotation :
#{in degrees.}
rotation = 0

use size=

#F[Decorations;gtk-orientation-portrait]
frame_deco=

#o+ Choose a decoration theme for this desklet :
#{Choose the 'personnal' one to define your own decorations below.}
decorations = default

#v
sep_deco =

#S+ Background image :
#{It's an image that will be displayed below the drawings, like a frame for exemple. Let empty to not use any.}
bg desklet =
#e+[0;1] Background tansparency :
bg alpha = 1
#i+[0;256] Left offset :
#{in pixels. Use this to adjust the left position of the drawings.}
left offset = 0
#i+[0;256] Top offset :
#{in pixels. Use this to adjust the top position of the drawings.}
top offset = 0
#i+[0;256] Right offset :
#{in pixels. Use this to adjust the right position of the drawings.}
right offset = 0
#i+[0;256] Bottom offset :
#{in pixels. Use this to adjust the bottom position of the drawings.}
bottom offset = 0
#S+ Foreground image :
#{It's an image that will be displayed above the drawings, like a reflect for exemple. Let empty to not use any.}
fg desklet =
#e+[0;1] Foreground tansparency :
fg alpha = 1

#[gtk-preferences]
[Configuration]

#i[20;180] Interval of time between 2 checks :
#{in seconds.}
check interval = 30

use approx = false

use_dbus = false

#F[Display;gtk-dnd-multiple]
frame_info=

#S+ Icon's filename when on battery :
battery icon = 
#S+ Icon's filename when on charge :
charge icon = 
#r+[nothing;zoom;transparency;draw bar] Effect to apply on the icon according to charge :
effect=0


#b+ Use a gauge instead of icons ?
use gauge = false
#g+ List of available gauges :
theme = battery

#r+[nothing;charge;time] Information to be displayed on the icon :
quick-info_type=2


#F[Alert;gtk-dialog-info]
frame_alert=

#b Alert when battery charged ?
high battery=true
#u Use a sound :
#{Let it empty to have no sound}
sound_2 =
#b Alert when low battery ?
low battery=true
#i[1;90] Battery level :
#{in percent.}
low value = 15
#u Use a sound :
#{Let it empty to have no sound}
sound_1 =
#b Alert when battery charge is critical ?
#{When battery level is under 4%}
critical battery=true
#u Use a sound :
#{Let it empty to have no sound}
sound_0 =

#b Animate icon during the alert ?
battery witness=true
#a+ Animation during alert :
battery_animation=pulse

