msgid "Desired icon size for this applet"
msgstr "Taille d'icône souhaitée pour cette applet"

#: ../data/messages:5
msgid "width :"
msgstr "largeur :"

#: ../data/messages:7
msgid "height :"
msgstr "hauteur :"

