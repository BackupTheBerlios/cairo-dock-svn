#: ../data/messages:1
msgid "Icon"
msgstr "Значок"

#: ../data/messages:3
msgid "Desired icon size for this applet"
msgstr "Определите размер значка для этого апплета"

#: ../data/messages:5
msgid "width :"
msgstr "ширина:"

#: ../data/messages:7
msgid "height :"
msgstr "высота:"

