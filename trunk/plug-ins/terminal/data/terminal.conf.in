#!en;@VERSION@

[Icon]
#F[Size of the icon you wish this applet has (in pixels)]
frame1 =
#i width :
width = 32
#i height :
height = 32

#F
frame1_fin =

#s Name of the icon as it will appear in its label in cairo-dock :
name = terminal

#S Icon's name :
icon = gnome-terminal

[Desklet]

can detach = false

#i[48;256] Desklet's dimension (width x height) :
width=256
#i[48;256] ...
height=196

#i Desklet's position (x ; y) :
x position=0
#i ...
y position=0

#b Is detached ?
initially detached=false

#b Keep below ?
keep below=false
#b Keep above ?
keep above=false
#b Keep on widget layer ?
on widget layer=false

[GUI]
#e[0;1] Terminal's transparency :
#{needs to restart the applet.}
terminal transparency=.84

#b Always on top ?
always on top=false

#c3[0;1] Foreground color (r, v, b):
foreground color= 1.; 1.; 1.

#c3[0;1] Background color (r, v, b):
background color= 0.; 0.; 0.

#I-[5;999] Size of the terminal (line x columns) :
nb lines = 25
#I-[5;999] ...
nb columns = 80

#k Shortkey to show the terminal
shortkey = <Ctrl>F1

#b Should the terminal be detached from the dock ?
is detached = FALSE
