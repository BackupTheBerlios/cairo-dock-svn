#!en;@VERSION@

[Icon]
#F[Size of the icon you wish this applet has (in pixels)]
frame1 =
#i width :
width = 32
#i height :
height = 32

#F
frame1_fin =

#s Name of the icon as it will appear in its label in cairo-dock :
name = terminal

#S Icon's name :
icon = gnome-terminal

[Desklet]

#i Desklet's position (x ; y) :
#{Depending on your WindowManager, you can move it with ALT + left_click}
x position=0
#i ...
y position=0

use size=false

#b Always draw icon in the dock ?
always draw icon=true

#b Is detached from the dock ?
initially detached=false

#b Keep below other windows ?
keep below=false
#b Keep above other windows ?
keep above=false
#b Keep on widget layer ?
#{for CompizFusion's "widget layer", set behaviour in Compiz to: (name=cairo-dock & type=utility)}
on widget layer=false

[GUI]
#e[0;1] Terminal's transparency :
#{needs to restart the applet.}
terminal transparency=.84

#c3[0;1] Foreground color (r, v, b):
foreground color= 1.; 1.; 1.

#c3[0;1] Background color (r, v, b):
background color= 0.; 0.; 0.

#I-[5;999] Size of the terminal (line x columns) :
nb lines = 25
#I-[5;999] ...
nb columns = 80

#k Shortkey to show the terminal
shortkey = <Ctrl>F1
