msgstr "Desklet"

#: ../data/messages:17
msgid "Desklet's dimension (width x height) :"
msgstr "Dimensione della desklet (larghezza x altezza) :"

#: ../data/messages:19
msgid "Desklet's position (x ; y) :"
msgstr "Posizione della desklet (x ; y) :"

#: ../data/messages:21
msgid "Is detached ?"
msgstr "E' staccata ?"

#: ../data/messages:23
msgid "Keep below ?"
msgstr "Mantenere al di sotto ?"

