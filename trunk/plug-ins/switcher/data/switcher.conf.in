#!en;@VERSION@

[Icon]
#F[Desired icon size for this applet]
frame1 = 
#i width :
width = 32
#i height :
height = 32

#F
frame1_ = 

#s Name of the icon as it will appear in its label in the dock :
name = switcher

#S Icon's name :
icon = workspaces.svg

[Desklet]

can detach = false

#i[48;256] Desklet's dimension (width x height) :
width=92
#i[48;256] ...
height=92

#i Desklet's position (x ; y) :
x position=0
#i ...
y position=0

#b Is detached ?
initially detached=false

#b Keep below ?
keep below=false
#b Keep above ?
keep above=false
#b Keep on widget layer ?
on widget layer=false

[Configuration]

#i Duration of dialogs :
#{in seconds. Set 0 to have never-ending dialogs.}
nombre de bureau = 8

#i Interval of time between 2 checks :
#{in minutes.}
taille horizontale = 8

#i[0;5] Number of days you want to forecast :
taille verticale = 1

#b Display current type of view on the icon instead of the default one ?
complexe view = true

#i Interval of time between 2 checks :
#{in minutes.}
check interval = 15

#b Display current type of view on the icon instead of the default one ?
display numero desktop = false

#S Default icon :
#{Let empty to use the default one.}
default icon = 

#S Broken icon :
#{Let empty to use the default one.}
broken icon = 

#R1[] Name of the view used for the sub-dock :
#{Let it empty to use default sub-dock view.}/
renderer = 

#b Render desklet in 3D ?
3D desket = false
