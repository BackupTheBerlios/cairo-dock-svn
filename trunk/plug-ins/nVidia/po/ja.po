msgstr "秒単位"

#: ../data/messages:55
msgid "Delay between temperature checks :"
msgstr "温度チェックの間隔時間 :"

#: ../data/messages:57 ../data/messages:61 ../data/messages:67
msgid "in celcius."
msgstr "摂氏"

#: ../data/messages:59
msgid "Lower limit of core temperature :"
msgstr "コア温度の下限 :"

#: ../data/messages:63
msgid "Upper limit of core temperature :"
msgstr "コア温度の上限 :"

#: ../data/messages:65
msgid "Alert when core temperature is high ?"
msgstr "コア温度が高い時に警告？"

#: ../data/messages:69
msgid "Alert limit of core temperature :"
msgstr "コア温度のリミット警告 :"

#: ../data/messages:71
msgid "Play a sound during the alert ?"
msgstr "警告中に音を鳴らす？"

#: ../data/messages:73
msgid "Sound path :"
msgstr "音ファイルのパス :"

#: ../data/messages:75
msgid "Images"
msgstr "画像"

#: ../data/messages:77
