#!en;@VERSION@

[Icon]
#F[Size of the icon you wish this applet has (in pixels)]
frame1 =
#i width :
width = 32
#i height :
height = 32

#F
frame1_fin =

#s Name of the icon as it will appear in its label in cairo-dock :
name = systray

#S Icon's name :
icon = gnome-panel-notification-area

[Desklet]

#i[48;256] Desklet's dimension (width x height) :
width=256
#i[48;256] ...
height=196

#i Desklet's position (x ; y) :
x position=0
#i ...
y position=0

#b Is detached ?
initially detached=false

#b Keep below ?
keep below=false
#b Keep above ?
keep above=false
#b Keep on widget layer ?
on widget layer=false


[GUI]
#k Shortkey to toggle the systray
shortkey = <Ctrl>F2
