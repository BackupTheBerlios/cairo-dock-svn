#: ../data/messages:125
msgid "Name of an image for the 'setting' icon :"
msgstr "「設定」のアイコン画像の名前 :"

#: ../data/messages:129
msgid "Name of an image for the 'emerald' icon :"
msgstr "「Emerald」のアイコン画像の名前 :"

#: ../data/messages:133
