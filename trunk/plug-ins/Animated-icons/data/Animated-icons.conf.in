#!en;@VERSION@


#[@pkgdatadir@/icon-rotation.png]
[Rotation]

#b Use it ?
use = true

#I[0;5000] Duration of the animation :
#{In ms.}
duration = 1500

#b Repeat while icon is pointed ?
continue = false

#r+ [Normal;Cube;Capsule] Type of mesh :
mesh = 2

#c+4 color of the mesh :
color = 1;1;1;1



#[@pkgdatadir@/spot.png]
[Spot]

#b Use it ?
use = false

#I[0;5000] Duration of the animation :
#{In ms.}
duration = 1000

#b Repeat while icon is pointed ?
continue = true

#c+3 color of the spot :
spot color = .2;.2;1

#c+4 color of the halo :
halo color = 0;0;1;1

#F-[Rays;gtk-fullscreen]
frame_ray =

#c+3 1st color of gradation :
color1 = 0;1;.7

#c+3 2nd color of gradation :
color2 = .1;1;.1

#b+ Random colors ?
mystical = false

#I[10;500] Number of rays :
nb part = 300

#I[10;60] Rays size :
part size = 20

#e[1;10] Rays speed :
part speed = 5.


#[@pkgdatadir@/icon-wobbly.png]
[Wobbly]

#b Use it ?
use = false

#r[Horizontal stretch;Vertical stretch;Corner stretch] Initial stretch :
stretch = 0

#i[200;350] Spring constant :
spring cst = 300

#e[7;12] Friction :
friction = 8

#I[2;20] Number of points on the grid in one direction :
grid nodes = 10


#[@pkgdatadir@/icon-wave.png]
[Wave]

#b Use it ?
use = false

#I[0;1000] Duration of the animation :
#{In ms.}
duration = 200

#b Repeat while icon is pointed ?
continue = false

#e[0.1;2] Width of the wave :
width = 1.25

#e[0.01;.4] Amplitude of the wave :
amplitude = .14

