msgid "Speed :"
msgstr "Vitesse :"

#: ../data/messages:13
msgid "Number of round per second."
msgstr "Nombre de tour par seconde."

#: ../data/messages:15
msgid "Rotation speed :"
msgstr "Vitesse de rotation :"
